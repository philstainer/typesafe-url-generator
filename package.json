{
  "name": "typesafe-url-generator",
  "version": "1.1.0",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./create-url-generator": "./dist/create-url-generator.js",
    "./q-url": "./dist/q-url.js"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "description": "A simple API for creating a type-safe urls",
  "scripts": {
    "build": "tsup",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "test.types": "vitest --typecheck.enabled --run",
    "test.types:watch": "vitest --typecheck.enabled --watch",
    "release": "pnpm run build && changeset publish"
  },
  "keywords": [
    "url",
    "typescript"
  ],
  "author": "Phil Stainer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/philstainer/typesafe-url-generator.git"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.4",
    "@changesets/cli": "^2.26.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "pnpm": ">=9",
    "node": ">=22"
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
}
